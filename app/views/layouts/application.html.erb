<!DOCTYPE html>
<html>
  <head>
    <title>AwsSteps</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
<nav class="navbar navbar-default navbar-collapse-top">
  <div class="container">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span> 
      </button>
      <strong class="text-success"><%= link_to "AWS installation", root_path , :class => "btn btn-success" %></strong>
      </div>
      <div class="collapse navbar-collapse" id="myNavbar">
        <ul class="nav navbar-nav navbar-right">
          <li><%= link_to 'HOME', homes_path %></li>
          <li><%= link_to 'AWS', installations_path %></li>
          <li><%= link_to "My Blog", "http://gopikrishnanayini.herokuapp.com/", data: { confirm: "Are you sure?" } %></li>
          <% if user_signed_in? %>
          <li><%= link_to "Sign Out", destroy_user_session_path, :method => :delete %></li>
          <% else %>
          <li>
            <%= link_to "Sign In", new_user_session_path%>
          </li>
          <% end %>
      </ul>
    </div>
  </div>
</nav>
  <body>
    <%= yield %>
  </body>
</html>
